<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="res_company_form__l10n_ve_view_for" model="ir.ui.view">
            <field name="name">Datos de la Empresa</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="priority">30</field>
            <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Información Nómina" name="nomina_info">
                    <group string="Variables para el calculo de Nómina">
                        <field name="ivss_id"/>
                        <field name="numero_patronal"/>
                        <field name="representante_legal_ivss"/>
                        <field name="banavih_id"/>
                        <field name="cesta_ticket"/>
                        <field name="por_riesgo"/>
                        <field name="por_aporte_sso"/>
                        <field name="por_empleado_rpe"/>
                        <field name="por_empresa_rpe"/>
                        <field name="por_empresa_lph"/>
                        <field name="por_empleado_lph"/>
                        <field name="nro_cuenta_faov"/>
                        <field name="salario_minimo"/>
                        <field name="dias_utilidades"/>
                        <field name="periodo_prestaciones"/>
                    </group>
                </page>
            </xpath>
            </field>
        </record>
</odoo>

